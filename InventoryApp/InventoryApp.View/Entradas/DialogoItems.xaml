<UserControl x:Class="InventoryApp.View.Entradas.DialogoItems"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="410" d:DesignWidth="607">
    <Grid>
        <Label Content="Agregar Items" FontFamily="Arial Rounded MT" FontSize="17" FontStretch="Normal" Foreground="#848484" Height="35" HorizontalAlignment="Right" Margin="0,12,241,0" Name="label1" VerticalAlignment="Top" Width="135" />
        <Label Content="Número de Serie:" FontFamily="Arial Rounded MT" FontSize="11.5" FontStretch="Normal" Foreground="#848484" Height="28" HorizontalAlignment="Right" Margin="0,53,474,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" Margin="139,53,297,0" Name="textBox1" VerticalAlignment="Top" />
        <DataGrid AutoGenerateColumns="False" Height="272" Margin="12,87,12,0" Name="dataGrid1" VerticalAlignment="Top" />
        <Button Command="{Binding AddItemCommand, Mode=OneWay}" Content="ACEPTAR" Height="23" Margin="118,372,414,0" Name="btnAgregar" VerticalAlignment="Top" Width="75" />
        <Button Content="CANCELAR" Height="23" Margin="398,372,134,0" Name="btnCancelar" VerticalAlignment="Top" Width="75" />
    </Grid>
</UserControl>
