            <Border BorderBrush="Silver" BorderThickness="3" Height="3" Name="border4" Width="587" Margin="0,390,10,468" HorizontalAlignment="Right"></Border>
            <Border BorderBrush="Silver" BorderThickness="3" Height="3" Name="border5" Margin="0,276,8,582" HorizontalAlignment="Right" Width="587" />
