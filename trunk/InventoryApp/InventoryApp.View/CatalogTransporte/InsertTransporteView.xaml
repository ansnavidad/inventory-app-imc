<Window x:Class="InventoryApp.View.InsertTransporteView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        DataContext="{DynamicResource insertTransporteViewModel}"
        Title="InsertTransporteView" Height="300" Width="300">

    <Grid DataContext="{StaticResource insertTransporteViewModel}"  >
        <TextBox  Text="{Binding Transporte.TransporteName, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Height="23" HorizontalAlignment="Left" Margin="87,71,0,0" Name="textBox2" VerticalAlignment="Top" Width="120" />
        <Button Content="AGREGAR" Height="23" HorizontalAlignment="Stretch" Margin="109,149,95,89" Name="btnAgregar" VerticalAlignment="Stretch" Width="75" Command="{Binding AddItemCommand,Mode=OneWay}" Click="btnAgregar_Click" />
        <ComboBox SelectedItem="{Binding Transporte.TipoEmpresa,Mode=TwoWay}" ItemsSource="{Binding CatalogTipoEmpresaModel.TipoEmpresas, Mode=OneWay}" DisplayMemberPath="TIPO_EMPRESA_NAME" SelectedValuePath="UNID_TIPO_EMPRESA" Height="23" HorizontalAlignment="Left" Margin="87,112,0,0" Name="comboBox1" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
