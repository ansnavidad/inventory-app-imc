<Window x:Class="InventoryApp.View.Recibo.DlgAddMovimientoDetalleView"
        xmlns:my="clr-namespace:InventoryApp.View.Recibo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DlgAddMovimientoDetalleView" Height="327" Width="635">
    <DockPanel>
        <ContentControl HorizontalAlignment="Stretch" Name="contentControl1" VerticalAlignment="Stretch" DockPanel.Dock="Top">
            <my:AddMovimientoDetalleView />
        </ContentControl>
        <DockPanel HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" DockPanel.Dock="Bottom" >
            <Button Content="Agregar" Height="23" Width="75" HorizontalAlignment="Left" Command="{Binding AddMovimientoDetalleCmd}" Click="Button_Click" />
            <Button Content="Cancelar" Height="23"  Width="75" HorizontalAlignment="Right" Margin="0,0,0,0" />
        </DockPanel>
    </DockPanel>
</Window>
