<Window x:Class="InventoryApp.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="AUTO" Width="AUTO" xmlns:my="clr-namespace:InventoryApp.View.CatalogProveedor">
    <Grid >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="180" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="70" />
            <RowDefinition Height="40" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <ScrollViewer Background="#f0f0f0" Grid.Row="2" Grid.Column="1" VerticalScrollBarVisibility="Visible"  Margin="0,10,0,0" Height="auto" Width="auto"  
                      HorizontalAlignment="Stretch" Name="svContainer" HorizontalScrollBarVisibility="Visible">
            <!--<StackPanel Name="sp" Height="AUTO" Width="AUTO" />-->
            <ContentControl>
                <!--<vm:CatalogProveedorViewModel/>-->
            </ContentControl>
        </ScrollViewer>

        <Grid Grid.Row="0" >
            <Grid.Background>
                <ImageBrush ImageSource="Images/header.png" />
            </Grid.Background>
            <Image Source="Images/Logo.png" Width="150" Height="50" Margin="0,0,0,0"></Image>
        </Grid>

        <Grid Grid.Column="1" Grid.Row="0">
            <Grid.Background>
                <ImageBrush ImageSource="Images/header.png" />
            </Grid.Background>
            <Label Content="elr.tracking@elara.com" HorizontalContentAlignment="Right" Height="28" HorizontalAlignment="Stretch" Margin="263,-1,13,0" Name="label1" VerticalAlignment="Stretch" />
        </Grid>

        <Grid Grid.Row="2"  HorizontalAlignment="Stretch" Margin="2,3,0,11" Name="grid2" Height="AUTO" VerticalAlignment="Stretch">
            <StackPanel  HorizontalAlignment="Stretch" Margin="-4,7,-5,-7" Name="stackPanel1" VerticalAlignment="Stretch">
                <Border Background="#d7e9f9" HorizontalAlignment="Left" Height="30" Width="182">
                </Border>
                <Expander Header="General" Margin="20,-26,0,0" Name="expander1" FontFamily="Arial" FontSize="14" Foreground="#6d7072">
                </Expander>
                <Border Background="#d7e9f9" HorizontalAlignment="Left" Height="30" Width="182">
                </Border>
                <Expander Header="Movimientos" Foreground="#6d7072" Margin="20,-28,0,0" Name="expander2" Background="Transparent" FontFamily="Arial" FontSize="14">
                    <StackPanel HorizontalAlignment="Left" Margin="0,10,0,0" Name="stackPanel3" VerticalAlignment="Top" Width="AUTO">
                        <Button HorizontalContentAlignment="Left" Name="Recibo_Almacen" Margin="0,0,0,4" Height="25" 
                                    Content="Recibo Almacen" Foreground="Black" TextBlock.TextAlignment="Left"  Width="160" 
                                    Background="Transparent" BorderBrush="Transparent" BorderThickness="1" VerticalAlignment="Center" 
                                    HorizontalAlignment="Left" FontSize="12">
                        </Button>
                        <Button HorizontalContentAlignment="Left" Name="Entradas" Height="25" Foreground="Black" Width="160" 
                                    Background="Transparent" TextBlock.BaselineOffset="0" Content="Entradas" BorderBrush="Transparent" 
                                    BorderThickness="1" VerticalAlignment="Center" HorizontalAlignment="Left" FontSize="12" >
                        </Button>
                        <Button HorizontalContentAlignment="Left" Name="Salidas" Height="25" Content="Salidas" Foreground="Black" 
                                    Width="160" Background="Transparent" TextBlock.TextAlignment="Left"  BorderBrush="Transparent" 
                                    BorderThickness="1" VerticalAlignment="Center" HorizontalAlignment="Left" FontSize="12">
                        </Button>
                    </StackPanel>
                </Expander>
                <TreeView Name="CatalogosTree" Margin="2,0,5,0" Width="auto">
                    <TreeViewItem Header="Catálogos" Margin="15,0,0,0">
                        <TreeViewItem Name="CatalogArticulo" Header="Artículo"></TreeViewItem>
                        <TreeViewItem Name="CatalogBanco" Header="Banco"></TreeViewItem>
                        <TreeViewItem Name="CatalogCategoria" Header="Categoría"></TreeViewItem>
                        <TreeViewItem Name="CatalogCiudad" Header="Ciudad"></TreeViewItem>
                        <TreeViewItem Name="CatalogDepartamento" Header="Departamento"></TreeViewItem>
                        <TreeViewItem Name="CatalogEmpresa" Header="Empresa"></TreeViewItem>
                        <TreeViewItem Name="CatalogEquipo" Header="Equipo"></TreeViewItem>
                        <TreeViewItem Name="CatalogItemStatus" Header="Item Status"></TreeViewItem>
                        <TreeViewItem Name="CatalogMarca" Header="Marca"></TreeViewItem>
                        <TreeViewItem Name="CatalogMedioEnvio" Header="Medio de Envió"></TreeViewItem>
                        <TreeViewItem Name="CatalogModelo" Header="Modelo"></TreeViewItem>
                        <TreeViewItem Name="CatalogMoneda" Header="Moneda"></TreeViewItem>
                        <TreeViewItem Name="CatalogPais" Header="País"></TreeViewItem>
                        <TreeViewItem Name="CatalogProveedor" Header="Proveedor"></TreeViewItem>
                        <TreeViewItem Name="CatalogProveedorCuenta" Header="Proveedor Cuenta"></TreeViewItem>
                        <TreeViewItem Name="CatalogProyecto" Header="Proyecto"></TreeViewItem>
                        <TreeViewItem Name="CatalogSolicitante" Header="Solicitante"></TreeViewItem>
                        <TreeViewItem Name="CatalogTerminoEnvio" Header="Termino de Envió"></TreeViewItem>
                        <TreeViewItem Name="CatalogTipoCotizacion" Header="Tipo de Cotización"></TreeViewItem>
                        <TreeViewItem Name="CatalogTipoEmpresa" Header="Tipo de Empresa"></TreeViewItem>
                        <TreeViewItem Name="CatalogTipoMovimiento" Header="Tipo de Movimiento"></TreeViewItem>
                        <TreeViewItem Name="CatalogTipoPedimento" Header="Tipo de Pedimento"></TreeViewItem>
                        <TreeViewItem Name="CatalogTipoTransporte" Header="Tipo de Transporte"></TreeViewItem>
                    </TreeViewItem>
                </TreeView>
            </StackPanel>
        </Grid>
        <Grid Grid.Column="1" Grid.Row="1" HorizontalAlignment="Stretch" Margin="5,0,0,0" Name="grid3" VerticalAlignment="Stretch">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" Margin="1,6,0,-2" Name="stackPanel2" VerticalAlignment="Stretch">

            </StackPanel>
        </Grid>
        <Border BorderBrush="#FF1626F2" BorderThickness="1" Grid.Row="2" Height="3" HorizontalAlignment="Stretch" Margin="0,9,0,0" 
                Name="border1" VerticalAlignment="Top" Width="auto" Grid.ColumnSpan="2">
        </Border>
        <Expander Grid.Row="1" Header="Inventarios" Height="26" HorizontalAlignment="Center" Margin="0,10,0,0" Name="expander3" 
                  VerticalAlignment="Top" Foreground="#FF0000F7" FontWeight="Medium" FontStretch="Normal" FontFamily="Arial" FontSize="18">
            <Grid />
        </Expander>
    </Grid>
</Window>
